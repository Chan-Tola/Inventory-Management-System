# Git
.git/
.gitignore
README.md

# Node.js (if you have any frontend assets)
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Laravel cache and logs (safe to ignore)
storage/framework/cache/*
storage/framework/views/*
storage/logs/*
!storage/framework/cache/.gitkeep
!storage/framework/views/.gitkeep
!storage/logs/.gitkeep

# Development files (KEEP .env files!)
.env.example
.env.testing
.phpunit.result.cache
.phpunit.cache

# Docker files (don't copy Docker files into container)
docker-compose.yml
docker-compose.override.yml
Dockerfile
.dockerignore

# IDE files
.idea/
.vscode/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Composer (but keep composer.json/lock)
composer.phar
/vendor/
!composer.json
!composer.lock

# Tests
/tests/
/coverage/

# Frontend build files (if any)
/public/hot
/public/storage
/storage/*.key
/bootstrap/cache/*